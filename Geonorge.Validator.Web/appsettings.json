{
  "StaticDataConfig": {
    "Arealformål": {
      "Url": "http://skjema.geonorge.no/SOSITEST/produktspesifikasjon/Reguleringsplanforslag/5.0/RpArealform%c3%a5l.xml",
      "FileName": "arealformål.json",
      "CacheDays": 30
    },
    "FeltnavnArealformål": {
      "Url": "http://skjema.geonorge.no/SOSITEST/produktspesifikasjon/Reguleringsplanforslag/5.0/RpFeltnavnArealform%c3%a5l.xml",
      "FileName": "feltnavn.json",
      "CacheDays": 30
    },
    "Hensynskategori": {
      "Url": "https://register.geonorge.no/api/plansak/hensynskategori.xml",
      "FileName": "hensynskategori.json",
      "CacheDays": 30
    }
  },
  "Serilog": {
    "Using": [ "Serilog.Sinks.Console" ],
    "MinimumLevel": {
      "Default": "Debug",
      "Override": {
        "Microsoft": "Warning",
        "Microsoft.EntityFrameworkCore.Storage.Internal.RelationalCommandBuilderFactory": "Warning",
        "Microsoft.EntityFrameworkCore.Storage.IRelationalCommandBuilderFactory": "Warning",
        "System": "Warning"
      }
    },
    "WriteTo": [
      {
        "Name": "Console",
        "Args": {
          "outputTemplate": "{Timestamp:yyyy-MM-dd HH:mm:ss} [{Level}] [Correlation ID: {CorrelationId}] {Message:lj}{NewLine}{Exception}"
        }
      }
    ],
    "Enrich": [ "FromLogContext", "WithMachineName", "WithThreadId" ]
  },
  "AllowedHosts": "*"
}
